{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "baseUrl": "./",
    "types": ["cypress", "node"],
    "paths": {
      "@pages/*": ["cypress/pages/*"],
      "@e2e/*": ["cypress/e2e/*"],
      "@constants/*": ["cypress/constants/*"],
      "@data/*": ["cypress/data/*"],
      "@typing/*": ["cypress/types/*"],
      "@fixtures/*": ["cypress/fixtures/*"],
      "@utils/*": ["cypress/utils/*"]
    }
  },
  "include": ["cypress/**/*.ts"]
}
